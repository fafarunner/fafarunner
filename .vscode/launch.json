    {
      "name": "ios",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file",
        ".env.dev.local",
      ]
    },
    {
      "name": "desktop & web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file",
        ".env.dev.local"
      ]
    }
  ]
